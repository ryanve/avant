{
  "name": "avant",
  "description": "Cross-browser JavaScript events",
  "version": "0.2.0",
  "homepage": "https://github.com/ryanve/avant",
  "license": "MIT",
  "author": "Ryan Van Etten",
  "keywords": ["event", "events", "dom", "browser"],
  "main": "./avant.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanve/avant.git"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "aok": "~1.8.1",
    "universal": "0.1.0",
    "grunt-contrib-uglify": "~0.6.0",
    "grunt-contrib-concat": "~0.4.0",
    "grunt-contrib-jshint": "~0.10.0"
  },
  "jshintConfig": {
    "asi": false,
    "boss": true,
    "browser": true,
    "debug": true,
    "devel": true,
    "eqnull": true,
